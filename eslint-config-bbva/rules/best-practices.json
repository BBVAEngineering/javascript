{
  "rules": {
    "accessor-pairs": 0,
    "block-scoped-var": 2,
    "complexity": [1, 9],
    "consistent-return": 1,
    "curly": [1, "multi-line"],
    "default-case": 2,
    "dot-location": [1, "property"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 1,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 1,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 1,
    "no-magic-numbers": [1, { "ignore": [-1, 0, 1, 2] }],
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 1,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-process-env": 1,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": [1, {
      "allowShortCircuit": true,
      "allowTernary": true }
    ],
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 2,
    "no-warning-comments": [1, { "terms": ["fixme", "xxx"], "location": "anywhere" }],
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 1,
    "wrap-iife": [1, "outside"],
    "yoda": 1
  }
}
