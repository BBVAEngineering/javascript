{
  "rules": {
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": [1, "always"],
    "prefer-const": 1,
    "prefer-reflect": 0,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-yield": 1,
    "no-confusing-arrow": [1, { "allowParens": false }]
  }
}
